{
  "name": "@maxmind/geoip2-node",
  "version": "3.2.0",
  "description": "Node.js API for GeoIP2 webservice client and database reader",
  "main": "dist/src/index.js",
  "homepage": "https://github.com/maxmind/GeoIP2-node",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxmind/GeoIP2-node.git"
  },
  "author": "MaxMind",
  "license": "Apache-2.0",
  "private": false,
  "keywords": [
    "geoip",
    "geoip2",
    "geoip precision",
    "geoip2 precision",
    "geolite",
    "geolite2",
    "maxmind",
    "maxminddb",
    "mmdb"
  ],
  "files": [
    "dist"
  ],
  "types": "dist/src/index.d.ts",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@types/ip6addr": "^0.2.3",
    "@types/jest": "^27.0.1",
    "@types/lodash.set": "^4.3.4",
    "@types/node": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-prettier": "^4.0.0",
    "gh-pages": "^3.0.0",
    "husky": "^7.0.2",
    "jest": "^27.0.1",
    "lint-staged": "^11.0.0",
    "nock": "^13.0.2",
    "pinst": "^2.1.4",
    "prettier": "^2.0.3",
    "ts-jest": "^27.0.1",
    "typedoc": "^0.22.4",
    "typescript": "^4.4.3"
  },
  "lint-staged": {
    "*.json": [
      "prettier --parser json --write"
    ],
    "*.ts": [
      "eslint . --ext .js,.ts --fix-dry-run"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "build:docs": "rimraf docs && typedoc src/index.ts --out docs --exclude \"**/*.spec.ts\" --readme README.md && ./bin/sanitize-doc-refs.sh",
    "deploy:docs": "gh-pages -d docs",
    "lint": "eslint . --ext .js,.ts --fix-dry-run ",
    "prettier:json": "prettier --parser json --write '**/*.json'",
    "test": "jest",
    "test:watch": "jest --watch",
    "postinstall": "husky install",
    "prepublishOnly": "npm run build && npm test && npm run lint && npm run build:docs && npm deploy:docs && pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "camelcase-keys": "^7.0.0",
    "ip6addr": "^0.2.3",
    "lodash.set": "^4.3.2",
    "maxmind": "^4.2.0"
  }
}
